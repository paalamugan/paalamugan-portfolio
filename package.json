{
    "name": "paalamugan-portfolio",
    "description": "Paalamugan Portfolio",
    "version": "1.0.0",
    "author": "Paalamugan",
    "scripts": {
        "serve": "gatsby build && NODE_ENV=production gatsby serve",
        "build": "gatsby build",
        "dev": "gatsby develop",
        "clean": "gatsby clean",
        "lint": "eslint ./src --ext .js --cache",
        "posttest": "npm run lint",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "test:e2e": "start-server-and-test dev http://localhost:8000 cy:open",
        "test:e2e:ci": "start-server-and-test dev http://localhost:8000 cy:run",
        "test": "NODE_ENV=testing mocha -- $(find test -name '*.test.js') && npm run test:e2e:ci",
        "preship": "npm run test",
        "ship": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then npm version && git push --follow-tags; fi",
        "main": "git pull -r origin main --autostash && git pull -r origin main --autostash && npm install",
        "format": "prettier --write \"**/*.js\""
    },
    "dependencies": {
        "algolia-html-extractor": "0.0.1",
        "animate.css": "^4.1.1",
        "caniuse-lite": "^1.0.30001264",
        "emailjs-com": "^2.6.4",
        "gatsby": "^3.13.0",
        "gatsby-image": "^3.1.0",
        "gatsby-plugin-advanced-sitemap": "^2.0.0",
        "gatsby-plugin-algolia": "^0.22.1",
        "gatsby-plugin-catch-links": "^3.12.0",
        "gatsby-plugin-force-trailing-slashes": "^1.0.5",
        "gatsby-plugin-manifest": "^3.12.0",
        "gatsby-plugin-offline": "^4.12.0",
        "gatsby-plugin-postcss": "^4.12.0",
        "gatsby-plugin-react-helmet": "^4.12.0",
        "gatsby-plugin-react-svg": "^3.0.1",
        "gatsby-plugin-remove-serviceworker": "^1.0.0",
        "gatsby-plugin-sass": "^4.12.0",
        "gatsby-plugin-sharp": "^3.12.0",
        "gatsby-plugin-sitemap": "^3.1.0",
        "gatsby-remark-autolink-headers": "^3.1.0",
        "gatsby-remark-code-titles": "^1.1.0",
        "gatsby-remark-embed-snippet": "^5.1.0",
        "gatsby-remark-external-links": "0.0.4",
        "gatsby-remark-images": "^5.0.0",
        "gatsby-remark-prismjs": "^4.1.0",
        "gatsby-source-filesystem": "^3.12.0",
        "gatsby-transformer-remark": "^4.9.0",
        "gatsby-transformer-sharp": "^3.12.0",
        "gatsby-transformer-yaml": "^3.12.0",
        "jquery": "^3.6.0",
        "node-sass": "^5.0.0",
        "prismjs": "^1.25.0",
        "react": "^17.0.1",
        "react-autosuggest": "^10.1.0",
        "react-dom": "^17.0.1",
        "react-helmet": "^6.1.0",
        "react-images": "^1.2.0-beta.6",
        "react-instantsearch-dom": "^6.12.1",
        "react-modal": "^3.14.3",
        "react-responsive-carousel": "^3.2.20",
        "tocbot": "^4.13.1"
    },
    "keywords": [
        "gatsby",
        "portfolio", 
        "react"
    ],
    "license": "MIT",
    "devDependencies": {
        "@testing-library/cypress": "^7.0.4",
        "algoliasearch": "^4.8.6",
        "autoprefixer": "^10.2.5",
        "babel-eslint": "^10.1.0",
        "cssnano": "^4.1.10",
        "cypress": "^6.8.0",
        "eslint": "^7.22.0",
        "eslint-plugin-react": "^7.22.0",
        "gatsby-cli": "^3.1.0",
        "mocha": "^8.3.2",
        "postcss-color-mod-function": "^3.0.3",
        "postcss-css-variables": "^0.17.0",
        "postcss-custom-media": "^8.0.0",
        "postcss-custom-properties": "^11.0.0",
        "postcss-easy-import": "^3.0.0",
        "prettier": "^2.0.2",
        "should": "^13.2.3",
        "sinon": "^9.2.4",
        "start-server-and-test": "^1.12.1"
    },
    "engines": {
        "node": "v14.15.3",
        "npm": "6.14.9"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/paalamugan/paalamugan-portfolio"
    }
}
